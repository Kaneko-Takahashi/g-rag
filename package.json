{
  "name": "g-rag",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev:web": "yarn workspace web dev",
    "build:web": "yarn workspace web build",
    "dev:api": "cd apps/api && python -m uvicorn main:app --reload --port 8000",
    "install:all": "yarn install && cd apps/api && pip install -r requirements.txt"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}

